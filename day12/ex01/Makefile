NAME = ft_cat

SRCS = ft_cat.c

OUT = ft_cat.o

all:
	gcc -Wall -Wextra -Werror $(SRCS) -o $(NAME)
clean:
	rm -f $(OUT)

fclean:	clean
	rm -f $(NAME)
